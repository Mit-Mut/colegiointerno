<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="inv_sur_inherit_form" model="ir.ui.view">
            <field name="name">inv.surcharge.inherit.form</field>
            <!-- Se cambio account.invoice -->
            <field name="model">account.move</field>
            <!-- Se cambio ref="account.invoice_form" -->
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <field name="team_id" position="after">
                    <!-- Se modifico [('type', '!=', 'out_invoice')]-->
                    <field name="surcharge_related_inv" attrs="{'invisible': [('move_type', '!=', 'out_invoice')]}"/>
                </field>
            </field>
        </record>


    </data>
</odoo>